{
    "name": "whisper",
    "productName": "Whisper",
    "version": "0.2.4",
    "description": "Cl*ely for Free",
    "main": "src/index.js",
    "scripts": {
        "setup": "npm install && cd whisper_web && npm install && npm run build && cd .. && npm start",
        "start": "npm run build:renderer && electron .",
        "package": "npm run build:all && electron-builder --dir",
        "make": "npm run build:renderer && electron-forge make",
        "build": "npm run build:all && electron-builder --config electron-builder.yml --publish never",
        "build:win": "npm run build:all && electron-builder --win --x64 --publish never",
        "publish": "npm run build:all && electron-builder --config electron-builder.yml --publish always",
        "lint": "eslint --ext .ts,.tsx,.js .",
        "postinstall": "electron-builder install-app-deps",
        "build:renderer": "node build.js",
        "build:web": "cd whisper_web && npm run build && cd ..",
        "build:all": "npm run build:renderer && npm run build:web",
        "watch:renderer": "node build.js --watch",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:unit": "jest tests/unit",
        "test:integration": "jest tests/integration",
        "test:e2e": "playwright test",
        "test:e2e:headed": "playwright test --headed",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:debug": "PWDEBUG=1 playwright test"
    },
    "keywords": [
        "whisper",
        "ai assistant",
        "real-time",
        "live summary",
        "contextual ai"
    ],
    "author": {
        "name": "Whisper Team"
    },
    "license": "GPL-3.0",
    "dependencies": {
        "@google/genai": "^1.8.0",
        "@google/generative-ai": "^0.24.1",
        "axios": "^1.10.0",
        "better-sqlite3": "^9.6.0",
        "cors": "^2.8.5",
        "dotenv": "^17.0.0",
        "electron-squirrel-startup": "^1.0.1",
        "electron-store": "^8.2.0",
        "electron-updater": "^6.6.2",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "node-fetch": "^2.7.0",
        "react-hot-toast": "^2.5.2",
        "react-markdown": "^10.1.0",
        "remark-gfm": "^4.0.1",
        "sharp": "^0.34.2",
        "validator": "^13.11.0",
        "wait-on": "^8.0.3",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@electron/fuses": "^1.8.0",
        "@electron/notarize": "^2.5.0",
        "@jest/globals": "^29.7.0",
        "@playwright/test": "^1.49.0",
        "@types/jest": "^29.5.11",
        "electron": "^30.5.1",
        "electron-builder": "^26.0.12",
        "electron-reloader": "^1.2.3",
        "esbuild": "^0.25.5",
        "jest": "^29.7.0",
        "jest-environment-node": "^29.7.0",
        "playwright": "^1.49.0",
        "prettier": "^3.6.2"
    },
    "optionalDependencies": {
        "electron-liquid-glass": "^1.0.1"
    }
}
