{
    "name": "whisper",
    "productName": "Whisper",
    "version": "0.2.4",
    "description": "Cl*ely for Free",
    "main": "main.cjs",
    "scripts": {
        "setup": "npm install && cd whisper_web && npm install && npm run build && cd .. && npm start",
        "start": "npm run build:renderer && electron .",
        "package": "npm run build:all && electron-builder --dir",
        "make": "npm run build:renderer && electron-forge make",
        "build": "npm run build:all && electron-builder --config electron-builder.yml --publish never",
        "build:win": "npm run build:all && electron-builder --win --x64 --publish never",
        "publish": "npm run build:all && electron-builder --config electron-builder.yml --publish always",
        "lint": "eslint --ext .ts,.tsx,.js .",
        "postinstall": "electron-builder install-app-deps",
        "build:renderer": "node build.js",
        "build:web": "cd whisper_web && npm run build && cd ..",
        "build:all": "npm run build:renderer && npm run build:web",
        "watch:renderer": "node build.js --watch"
    },
    "keywords": [
        "whisper",
        "ai assistant",
        "real-time",
        "live summary",
        "contextual ai"
    ],
    "author": {
        "name": "Whisper Team"
    },
    "license": "GPL-3.0",
    "dependencies": {
        "@google/genai": "^1.8.0",
        "@google/generative-ai": "^0.24.1",
        "classnames": "^2.5.1",
        "axios": "^1.10.0",
        "better-sqlite3": "^9.6.0",
        "cors": "^2.8.5",
        "dotenv": "^17.0.0",
        "electron-squirrel-startup": "^1.0.1",
        "electron-store": "^8.2.0",
        "electron-updater": "^6.6.2",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.2",
        "lucide-react": "^0.474.0",
        "keytar": "^7.9.0",
        "node-fetch": "^2.7.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hot-toast": "^2.5.2",
        "react-markdown": "^10.1.0",
        "remark-gfm": "^4.0.1",
        "sharp": "^0.34.2",
        "tailwind-merge": "^2.5.2",
        "tailwindcss-animate": "^1.0.7",
        "validator": "^13.11.0",
        "wait-on": "^8.0.3",
        "ws": "^8.18.0",
        "zustand": "^5.0.0"
    },
    "devDependencies": {
        "@electron-forge/cli": "^7.5.0",
        "@electron-forge/plugin-vite": "^7.5.0",
        "@electron/fuses": "^1.8.0",
        "@electron/notarize": "^2.5.0",
        "@types/node": "^22.5.4",
        "@types/react": "^18.3.5",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "electron": "^30.5.1",
        "electron-builder": "^26.0.12",
        "electron-reloader": "^1.2.3",
        "esbuild": "^0.25.5",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^4.6.2",
        "postcss": "^8.4.47",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.6",
        "shadcn-ui": "^0.9.5",
        "tailwindcss": "^3.4.13",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.6.3",
        "vite": "^5.4.9",
        "@vitejs/plugin-react": "^4.3.2"
    },
    "optionalDependencies": {
        "electron-liquid-glass": "^1.0.1"
    }
